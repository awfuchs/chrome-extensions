{
  "name": "Grouped Tab Lister",
  "action": {},
  "description": "List the tab groups in the current window, and the names of each contained tab group.",
  "version": "0.2",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "windows",
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
